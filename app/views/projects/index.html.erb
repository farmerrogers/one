<h1>Projects</h1>

<%= link_to "Start new Project", new_project_path, :class => "new_project" %>


<hr/>
<% for project in @projects %>
	<div class="projects">
		<br/>
		<h2><%= link_to project.title %></h2>
		<br/>
		<br/>
		<% for track in project.tracks %>
			<%= track.title %>
		<% end %>

	</div>
<% end %>



